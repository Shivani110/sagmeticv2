    header {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        background: #FBF7F7;
    }
@-webkit-keyframes fadeIn {
    0% {
      -webkit-transform: translateY(-100%);
      transform: translateY(-100%);
      transition: .4s
    }
    to {
      -webkit-transform: translateY(0);
      transform: translateY(0)
    }
  }
  @keyframes fadeIn {
    0% {
      -webkit-transform: translateY(-100%);
      transform: translateY(-100%);
      transition: .4s
    }
    to {
      -webkit-transform: translateY(0);
      transform: translateY(0)
    }
  }
  .site-header.fixed-header {
    -webkit-animation: fadeIn .75s ease-out forwards;
    animation: fadeIn .75s ease-out forwards;
  }
  header.site-header.fixed-header {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 99;
    box-shadow: 0px 10px 10px rgba(0,0,0,0.1);
}
header nav{
    padding: 21px 0px;
    background: #FBF7F7;
}
header a.nav-link {
    color: black !important;
    font-weight: 400;
    font-size: 14px;
    line-height: 17px;
    position: relative;
    padding: 6px 20px;
}
header a.nav-link:hover {
    color:#DE2D26 ;
    
}
 header li.nav-item.active {
    position: relative;
}
 header li.nav-item.active .nav-link {
    color: #DE2D26 !important;
} 
header li.nav-item.active .nav-link:before {
    position: absolute;
    content: "";
    height: 5px;
    width: 5px;
    left: 0;
    right: 0;
    top: 100%;
    margin: auto;
    background-color: #DE2D26;
    border-radius: 50%;
}
