.hover-box {
    position: relative;
}
 .project .hover-box .line1 {
    width: 100%;
    position: absolute;
    right: 0;
    top: 20px;
    height: 4px;
    width: 0px;
    background-color: #000000;
    transition: all 0.5s linear;
}
 .project .hover-box .line2 {
    width: 4px;
    position: absolute;
    left: 0;
    top: 20px;
    height: 0px;
    background-color: #000000;
    transition: all 0.5s linear;
}
  .project .hover-box .line3 {
    width: 100%;
    position: absolute;
    right: auto;
    top: auto;
    bottom: -20px;
    left: 0;
    height: 4px;
    width: 0px;
    background-color: #000000;
    transition: all 0.5s linear;
}
  .project .hover-box .line4 {
    width: 4px;
    position: absolute;
    left: auto;
    right: 0;
    top: 20px;
    bottom: 0;
    height: 0px;
    background-color: #000000;
    transition: all 0.5s linear;
}
 .hover-box .line1 {
    width: 100%;
    position: absolute;
    right: 0;
    top: 0;
    height: 4px;
    width: 0px;
    background-color: #000000;
