.box{
 
    transform: translate(0px,0px );
    transition: all 0.3s ease-in-out;
    margin-top: 25px;
}
.hover-box:hover .box{
    background: #FFFFFF;
    box-shadow: -33px 32px 64px rgba(24, 25, 34, 0.11);
    transform: translate(12px,-12px); 
}
.hover-box:hover .line4{
height: 100%;
transition-delay: 1.5s;
}
.hover-box:hover .line3{
width: 100%;
transition-delay: 1s;
}.hover-box:hover .line2{
height: 100%;
transition-delay: 0.5s;
}.hover-box:hover .line1{
width: 100%;
transition-delay: 0s;
}
@keyframes  linewidth {
    0%{
        width: 0px;

    }
    100%{
        width: 100%;
    }
    
}
@keyframes  lineheight {
    0%{
        height: 0px;

    }
    100%{
        height: 100%;
    }
    
}
